<div class="meeting_note_wrapper" style="padding:15px 10px 10px 20px;">
  <div class="meeting_note_left_column">
    <%= mn.updated_at.in_time_zone("Pacific Time (US & Canada)").strftime("%B %d, %Y %H:%M %p") %>
  </div>
  <div class="meeting_note_right_column">
    <%= simple_format(mn.description) %>
  </div>
</div>



